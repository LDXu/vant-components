{
  "name": "vant-components",
  "version": "0.0.1",
  "author": "Zehui Chan <jasonchenzehui@gmail.com>",
  "main": "dist/VantComponents.umd.min.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build:docs": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name VantComponents --dest ./dist/ ./lib/index.js",
    "build": "npm run build:lib && npm run build:docs",
    "publish": "npm publish",
    "unpublish": "npm unpublish --force",
    "release": "release-it && git push --follow-tags origin main"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:zehuichan/vant-components.git"
  },
  "dependencies": {
    "dayjs": "^1.10.6"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vant/markdown-loader": "^2.5.0",
    "@vant/touch-emulator": "^1.3.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "core-js": "^3.15.1",
    "highlight.js": "^10.7.3",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "release-it": "^14.10.0",
    "vant": "^2.12.24",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14"
  },
  "release-it": {
    "npm": {
      "publish": false
    },
    "git": {
      "push": false,
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    }
  }
}
